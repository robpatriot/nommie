# Postgres infra (docker)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres
POSTGRES_PORT=5432

# Owner and App DB role (runtime)
NOMMIE_OWNER_PASSWORD=topsecret
APP_DB_PASSWORD=changeme

# Connection strings (runtime uses nommie_app)
DATABASE_URL=postgres://nommie_app:changeme@localhost:5432/nommie

# Secret used to sign/verify backend JWTs
APP_JWT_SECRET=change-me-in-local

# CORS: comma-separated list of allowed origins
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
