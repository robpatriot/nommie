# Public values
NEXT_PUBLIC_APP_NAME=Nommie

# Auth.js (NextAuth v5) Configuration
# Copy this file to apps/frontend/.env.local and fill in your actual values
# Application URL (adjust in production)
AUTH_URL=http://localhost:3000

# Strong secret for signing/verification (generate with `npx auth secret`)
AUTH_SECRET=your_auth_secret_here

# Google OAuth credentials
# Get these from https://console.cloud.google.com/apis/credentials
AUTH_GOOGLE_ID=your_google_client_id_here
AUTH_GOOGLE_SECRET=your_google_client_secret_here
